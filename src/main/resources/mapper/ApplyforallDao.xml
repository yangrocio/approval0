<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aynu.dao.ApplyforallDao">
    <update id="updateApplyAll" statementType="STATEMENT">
        update userinfo set ${name}='${value}'
    </update>

    <update id="updateApplyByNumber" statementType="STATEMENT">
        update userinfo set ${name}='${value}' where number='${number}'
    </update>

</mapper>